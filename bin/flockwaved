#!/usr/bin/env python

import os
import sys

script_dir = os.path.abspath(os.path.dirname(sys.modules[__name__].__file__))

if os.path.isfile(os.path.join(script_dir, '..', 'flockwave', '__init__.py')):
	sys.path.insert(0, os.path.abspath(os.path.join(script_dir, '..')))

from flockwave.server.launcher import start
start()

